{
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "no-underscore-dangle":  0,
    "class-methods-use-this": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "import/no-unresolved": 0,
    "import/imports-first": 0,
    "no-unused-vars": 0,
    "import/no-extraneous-dependencies": 0,
    "prefer-const": 0,
    "max-len": 0
  }
}
